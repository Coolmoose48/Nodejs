<h2>Users Tabel</h2>
    <table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>username</th>
            <th>Zip Code</th>
            <th>Vip</th>
            <th>password</th>
           
        </tr>
  </thead >
    <tbody>
        <% users.forEach(user => { %>
            <tr>
                <td>
                    <%= user.fullname %>
      </td>
                <td>
                    <%= user.email %>
<td>
                    <%= user.username %>
      </td>
      </td>
                <td>
                    <%= user.zipCode %>
      </td>
<td>
                    <%= user.vip %>
      </td>
<td>
                    <%= user.password %>
      </td>
 
            </tr>
            <% }); %>
  </tbody>
</table >
